<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Items</title>
</head>
<body>
    <h1><%= title %></h1>
    <ul>
        <% items.forEach(function(item) { %>
            <a href="<%= item.url %>">
                <li><%= item.name %> - <%= item.price %></li>
            </a>
        <% }); %>
    </ul>
</body>
</html>
