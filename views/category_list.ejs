<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Category List</title>
</head>
<body>
    <h1><%= title %></h1>
    <ul>
        <% category_list.forEach(function(category) { %>
            <li><a href="<%= category.url %>"><%= category.name %></a></li>
        <% }); %>
    </ul>
</body>
</html>
